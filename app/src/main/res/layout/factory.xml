<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="ewulusen.kittykatt.factory">
    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical" >

    <TextView
        android:id="@+id/welcommeVar"
        android:layout_width="match_parent"
        android:layout_height="34dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:text="TextView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintBottom_toBottomOf="@+id/tabok" />
        <Button
            android:id="@+id/v1"
            android:layout_width="match_parent"
            android:layout_height="48dp"
            android:hint="vesz"
            android:text="vesz"
            android:textSize="10sp"
            app:layout_constraintHorizontal_bias="0.999"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintStart_toEndOf="@+id/imageView3"
            app:layout_constraintTop_toBottomOf="@+id/cicakDB" />

        <Button
            android:id="@+id/v2"
            android:layout_width="match_parent"
            android:layout_height="48dp"
            android:hint="vesz"
            android:text="vesz"
            android:textSize="10sp"
            app:layout_constraintHorizontal_bias="0.999"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintStart_toEndOf="@+id/imageView3"
            app:layout_constraintTop_toBottomOf="@+id/v1" />

        <Button
            android:id="@+id/v3"
            android:layout_width="match_parent"
            android:layout_height="48dp"
            android:layout_marginTop="8dp"
            android:hint="vesz"
            android:text="vesz"
            android:textSize="10sp"
            app:layout_constraintHorizontal_bias="0.999"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintStart_toEndOf="@+id/imageView3"
            app:layout_constraintTop_toBottomOf="@+id/v2" />

        <Button
            android:id="@+id/v4"
            android:layout_width="match_parent"
            android:layout_height="48dp"
            android:hint="vesz"
            android:text="vesz"
            android:textSize="10sp"
            app:layout_constraintHorizontal_bias="0.999"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintStart_toEndOf="@+id/imageView3"
            app:layout_constraintTop_toBottomOf="@+id/v3" />

        <Button
            android:id="@+id/v5"
            android:layout_width="match_parent"
            android:layout_height="48dp"
            android:hint="vesz"
            android:text="vesz"
            android:textSize="10sp"
            app:layout_constraintHorizontal_bias="0.999"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintStart_toEndOf="@+id/imageView3"
            app:layout_constraintTop_toBottomOf="@+id/v4" />
        <ImageView
            android:id="@+id/cicaImage"
            android:layout_width="200dp"
            android:layout_height="200dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintVertical_bias="0.0"
            app:srcCompat="@drawable/catkatt" />

        <ImageView
            android:id="@+id/back"
            android:layout_width="100dp"
            android:layout_height="100dp"
            app:srcCompat="@drawable/back" />
    </LinearLayout>

    </ScrollView>

</android.support.constraint.ConstraintLayout>
